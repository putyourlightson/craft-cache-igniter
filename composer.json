{
  "name": "putyourlightson/craft-cache-igniter",
  "description": "A global CDN and reverse proxy cache warmer.",
  "version": "1.1.0",
  "type": "craft-plugin",
  "homepage": "https://putyourlightson.com/plugins/cache-igniter",
  "license": "proprietary",
  "keywords": [
    "cache",
    "warm",
    "cdn",
    "global",
    "performance",
    "blitz"
  ],
  "require": {
    "php": "^8.0.2",
    "amphp/http-client": "^4.0",
    "amphp/parallel": "^1.0",
    "craftcms/cms": "^4.0"
  },
  "require-dev": {
    "craftcms/ecs": "dev-main",
    "craftcms/phpstan": "dev-main",
    "markhuot/craft-pest": "^1.0@alpha",
    "putyourlightson/craft-generate-test-spec": "dev-main"
  },
  "autoload": {
    "psr-4": {
      "putyourlightson\\cacheigniter\\": "src/"
    }
